{
  "version": "0.1.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "preview": "parcel ./example/index.html",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "release": "standard-version",
    "size": "size-limit",
    "husky-prepare": "husky install",
    "format": "prettier --write src",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "paynow-react",
  "author": "tate231",
  "module": "dist/paynow-react.esm.js",
  "size-limit": [
    {
      "path": "dist/paynow-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/paynow-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "husky": "^7.0.4",
    "parcel": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.10.2",
    "standard-version": "^9.3.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "commitlint": "^15.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "lint-staged": "^12.1.3"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "framer-motion": "4",
    "js-sha512": "^0.8.0",
    "qs": "^6.10.2"
  }
}
